                    if not self.focus and self.text.strip() != '': self.text = str(float(self.text))
                    if not self.focus and self.text.strip() == '': self.text = '0.0'
                    if not self.focus: self.cancel_selection()
            TextInput:
                id: price_dl
                input_filter: 'float'
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                text : '0.0'
                on_text: root.calc(self)
                write_tab: False
                on_focus:
                    if self.focus and self.text.strip() == '0.0': self.text = ''
                    if self.focus and self.text.strip() != '0.0': root.dl = True
                    if root.tl or root.eu: self.active = False
                    if not self.focus and self.text.strip() != '': self.text = str(float(self.text))
                    if not self.focus and self.text.strip() == '': self.text = '0.0'
                    if not self.focus: self.cancel_selection()
            TextInput:
                id: price_eu
                input_filter: 'float'
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                text : '0.0'
                on_text: root.calc(self)
                write_tab: False
