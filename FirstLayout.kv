<RemoveLabel>:
    id: remove_label
    size : self.texture_size
    size_hint_x: None
    width: "19dp"
    font_size: '23dp'
    halign : "center"
    valign : "middle"
    color: 1,0,0,1
    font_name: 'wingding.ttf'
    text: u'\uF0FD'

<PopupLayout>:
    top_layout: top_layout
    data_layout: data_layout
    escape_button: escape_button
    close_button: close_button
    delete_mode_button: delete_mode_button
    orientation: 'vertical'
    BoxLayout:
        orientation: 'vertical'
        id: top_layout
        size_hint_y: None
        height: "95dp"
        spacing: "1dp"
    ScrollView:
        size_hint : (1, 1)
        do_scroll_x: False
        do_scroll_y: True
        GridLayout:
            id: data_layout
            size_hint_y: None
            height: self.minimum_height
            cols: 1
            spacing: "1dp"
    BoxLayout:
        id: bottom_layout
        orientation: 'horizontal'
        size_hint_y: None
        height: "29dp"
        Button:
            id: delete_mode_button
            text: 'SIL'
            size_hint_x: None
            width: "101dp"
            background_color: (3,0,0,1)
        Widget:
        Button:
            size_hint_x: None
            width: "101dp"
            id: escape_button
            text: 'IPTAL'
            background_color: (0,1,0,1)
        Button:
            size_hint_x: None
            width: "101dp"
            id: close_button
            text: 'TAMAM'
            background_color: (0,1,0,1)
            on_press: self.disabled = True

<TextInputRowLayout>:
    id: text_input_row
    size_hint_y : None
    height : "37dp"
    id_lbl: id_lbl
    date_i: date_i
    type_i: type_i
    price_tl: price_tl
    price_dl: price_dl
    price_eu: price_eu
    quantity_i: quantity_i
    balance_tl: balance_tl
    balance_dl: balance_dl
    balance_eu: balance_eu
    payed_tl: payed_tl
    payed_dl: payed_dl
    payed_eu: payed_eu
    total_tl: total_tl
    total_dl: total_dl
    total_eu: total_eu
    tl: True
    dl: True
    eu: True
    client_id: 0
    BoxLayout:
        spacing: "1dp"
        BoxLayout:
            size_hint_x: None
            width: "35dp"
            canvas:
                Color:
                    rgba: (0.90,0.89,0.82,1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                id: id_lbl
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                # text : 'NO'
        BoxLayout:
            size_hint_x: None
            width: "87dp"
            canvas:
                Color:
                    rgba: (0.90,0.89,0.82,1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            DatePicker:
                id: date_i
                # text_size : self.size
                # halign : "center"
                # valign : "middle"
                # color: 0,0,0,1
                # write_tab: False
                # text: root.cur_date
                on_text: root.make_norm_date(self)
                # on_focus:
                #     if self.focus: self.select_all()
                #     if not self.focus: self.cancel_selection()

        BoxLayout:
            canvas:
                Color:
                    rgba: (0.90,0.89,0.82,1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            TextInput:
                id: type_i
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                write_tab: False
                # text : 'CINSI'
        BoxLayout:
            size_hint_x: None
            width: "75dp"
            canvas:
                Color:
                    rgba: (0.90,0.89,0.82,1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            TextInput:
                id: quantity_i
                input_filter: 'float'
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                text : '0.0'
                on_text: root.calc(self)
                write_tab: False
                on_focus:
                    if self.focus and self.text.strip() == '0.0': self.text = ''
                    if not self.focus and self.text.strip() != '': self.text = str(float(self.text))
                    if not self.focus and self.text.strip() == '': self.text = '0.0'
                    if not self.focus: self.cancel_selection()
        BoxLayout:
            canvas:
                Color:
                    rgba: (0.90,0.89,0.82,1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            TextInput:
                id: price_tl
                input_filter: 'float'
                text_size : self.size
                halign : "center"
                valign : "middle"
                color: 0,0,0,1
                text : '0.0'
                on_text:
                    root.calc(self)
                write_tab: False
                on_focus:
