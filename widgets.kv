<ClicablLabel@ButtonBehavior+Label>:


<ButImage@ButtonBehavior+AsyncImage>:
    FloatLayout:


<CustomWidthTabb@TabbedPanelItem>
    width: self.texture_size[0]
    padding: 10, 0
    size_hint_x: None


<Header>:
    GridLayout:
        cols: 1
        BoxLayout:
            size_hint_y : None
            height : 50
            spacing : 1
            padding : 1
            canvas:
                Color:
                    rgba: (0, 0.1, 0.25, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            BoxLayout:
                canvas:
                    Color:
                        rgba: norm_background_color 
                    Rectangle:
                        size: self.size
                        pos: self.pos
                size_hint_x : None
                width : 100
                AsyncImage:
                    source: 'eologo_bej.png'
            BoxLayout:
                canvas:
                    Color:
                        rgba: norm_background_color 
                    Rectangle:
                        size: self.size
                        pos: self.pos
                Label:
                    id: main_label
                    font_size: '25sp'
                    color: 0,0,0,1
                    text : u'\u00DCR\u00DCN YAP\u0049LAB\u0130L\u0130RL\u0130K RAPORU'
            BoxLayout:
                size_hint_x: None
                width: 130
                # canvas:
                #     Color:
                #         rgba: norm_background_color 
                #     Rectangle:
                #         size: self.size
                #         pos: self.pos
                BoxLayout:
                    orientation: 'vertical'
                    canvas:
                        Color:
                            rgba: norm_background_color 
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    Label:
                        text_size : self.size
                        halign : "left"
                        valign : "middle"
                        color: 0,0,0,1
                        text : " Rapor NO: 4587"
                        font_size: '15sp'
                    Label:
                        text_size : self.size
                        halign : "left"
                        valign : "middle"
                        color: 0,0,0,1
                        text : " Tarih:" #+ root.cur_date
                        font_size: '15sp'
